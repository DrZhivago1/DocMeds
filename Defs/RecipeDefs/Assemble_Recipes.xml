<Defs>

<!--=============== AI Persona Core Assembly ====================-->
    <RecipeDef>
        <defName>assemble_aipersonacore</defName>
        <label>Assemble AI Persona Core</label>
        <description>Make AI Persona Core< from nanites</description>
        <jobString>Make AI Persona Core< from nanites</jobString>
        <workSpeedStat>SmithingSpeed</workSpeedStat>
        <effectWorking>Cook</effectWorking>
        <soundWorking>Recipe_Machining</soundWorking>
        <workAmount>1000</workAmount>
        <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>DZ_Nanites</li>
                    </thingDefs>
                </filter>
                <count>150</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>ComponentSpacer</li>
                    </thingDefs>
                </filter>
                <count>5</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Plasteel</li>
                    </thingDefs>
                </filter>
                <count>50</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>DZ_Nanites</li>
                <li>ComponentSpacer</li>
                <li>Plasteel</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <AIPersonaCore>1</AIPersonaCore>
        </products>
        <researchPrerequisite>AdvancedFabrication</researchPrerequisite>
        <skillRequirements>
            <Crafting>20</Crafting>
            <Intellectual>20</Intellectual>
        </skillRequirements>
        <workSkill>Crafting</workSkill>
    </RecipeDef>

