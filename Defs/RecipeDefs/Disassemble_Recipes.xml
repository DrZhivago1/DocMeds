<Defs>

<!--=============== AI Persona Core disassembly ====================-->
    <RecipeDef>
        <defName>disassemble_aipersonacore</defName>
        <label>Disassemble AI Persona Core</label>
        <description>Make nanites from a AI Persona Core</description>
        <jobString>Make nanites from a AI Persona Core</jobString>
        <workSpeedStat>SmithingSpeed</workSpeedStat>
        <effectWorking>Cook</effectWorking>
        <soundWorking>Recipe_Machining</soundWorking>
        <workAmount>1000</workAmount>
        <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>AIPersonaCore</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Plasteel</li>
                    </thingDefs>
                </filter>
                <count>10</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Chemfuel</li>
                    </thingDefs>
                </filter>
                <count>10</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>AIPersonaCore</li>
                <li>Plasteel</li>
                <li>Chemfuel</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <DZ_Nanites>75</DZ_Nanites>
        </products>
        <researchPrerequisite>AdvancedFabrication</researchPrerequisite>
        <skillRequirements>
            <Crafting>15</Crafting>
            <Intellectual>15</Intellectual>
        </skillRequirements>
        <workSkill>Crafting</workSkill>
    </RecipeDef>

